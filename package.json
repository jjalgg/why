  "scripts": {
    "prestart": "babel src -d build && cd src && npx copy api/**/*.graphql ../build/api",
    "start": "cross-env NODE_ENV=production PORT=80 node ./node_modules/pm2/bin/pm2 start build/server.js -i -1",
    "dev": "nodemon --exec babel-node build/server.js",
    "poststart": "npx prisma generate && node ./node_modules/pm2/bin/pm2 logs",
    "migrate": "npx prisma migrate save --name init --experimental && npx prisma migrate up --experimental",
    "generate": "npx prisma generate",
    "build": "babel src -d build",
    "postbuild": "cd src && npx copy api/**/*.graphql ../build/api"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@prisma/cli": "^2.2.0"
  }
}
